<div class="container">
  <header>
    <img src="/assets/logo.png"/>

    <h1>Thoughtfold</h1>
    <h2>Clear your mind.</h2>

    <ul>
      <li><span><%= link_to "ABOUT", "#", id: "about-link" %></span></li>
      <li><span><%= link_to "GET THE APP", "#", id: "app-link" %></span></li>
      <li><span><%= link_to "SUBMIT", "#" %></span></li>
      <li><a href="#"><%= image_tag 'facebook.png', class: 'facebook' %></a></li>
      <li><a href="#"><%= image_tag 'twitter.png', class: 'twitter' %></a></li>
    </ul>
  </header>

  <div class="thoughts">
    <%= render partial: 'thought', collection: @thoughts %>
  </div>

  <%= render partial: 'info' %>

  <%= render partial: 'about' %>

  <%= render partial: 'app' %>
</div>